<table border="4px" class ="table">
  <tr>
    <th>User id</th>
    <th>User Name</th>
    <th>User Email</th>
    <th>IsAdmin</th>
    <th>Edit</th>
    <th>Delete</th>


  </tr>
  <% @users.each do |u| %>

      <tr>
        <td><%=u.id%></td><td><%=u.name%></td>  <td><%=u.email%></td> <td><%=u.admin%></td>
        <td><%= link_to ( image_tag "edit.jpg", width:'25', height:'25' ), edit_user_path(u) %></td>
        <td><%= link_to ( image_tag "delete.jpg", width:'25', height:'25' ), u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>

  <%end%>
</table>






